DateFormat = yyyy
Period     = from:1611 till:1911      # The 'till' value should be the day this timeline was last edited, to reflect the at the time current value
TimeAxis   = orientation:vertical
ScaleMajor = unit:year increment:10 start:1620
Define $dx = 20
Define $dy = -3
Colors=
 id:emp value:yellow legend:Emperor_of_China_Proper
